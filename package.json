{
  "name": "ts-lib-starter",
  "version": "0.0.1",
  "description": "A typescript lib starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/rencire/ts-lib-starter"
  },
  "author": "Eric Ren",
  "license": "GPL-3.0-only",
  "bugs": {
